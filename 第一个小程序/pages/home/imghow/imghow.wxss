/* pages/home/imghow/imghow.wxss */
.imglist{
   text-align: center;
   /* 给图添加阴影和圆角 */
   border-radius: 8px;
   box-shadow: 5px 8px 30px rgba(red, green, blue, alpha)
}
.imgicon,.imglist{
   width: 70rpx;
   height: 70rpx;
   margin: 20rpx;
}
.imgitem{
   width: 100rpx;
   height: 100rpx;
   margin: 40rpx;
}
.imglist1,.imgfull{
   width: 100%;
   height: 100px;
   border-radius: 8px;
}
.circle,.imglist3{
   width: 200px;
   height: 200px;
   border-radius: 100%;
   padding: 30px;
}